#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/lishizeng/Desktop/origin/nestjs-http-server-template/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/lishizeng/Desktop/origin/nestjs-http-server-template/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/ts-jest@28.0.8_mgg23zyyvjoe75wbzvxzqqpmne/node_modules/ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/ts-jest@28.0.8_mgg23zyyvjoe75wbzvxzqqpmne/node_modules/ts-jest/cli.js" "$@"
fi
